{
    "chmod": "0755",
    "base-path": "phar-build",
    "directories": ["src", "packages"],
    "directories-bin": ["bin"],
    "main": "bin/rector",
    "output": "rector.phar",
    "stub": true,
    "finder": [
        {
            "name": "*.php,*.yml",
            "exclude": ["Tests", "tests", "docs"],
            "in": "vendor"
        }
    ]
}

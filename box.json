{
    "chmod": "0755",
    "base-path": "build",
    "directories": ["src", "packages"],
    "directories-bin": ["bin"],
    "main": "bin/rector",
    "output": "rector.phar",
    "stub": "../packages/PharBuilder/Stub.php",
    "finder": [
        {
            "name": "*.{php,yml}",
            "exclude": ["Tests", "tests", "docs"],
            "in": "vendor"
        }
    ],
    "compression": "GZ"
}

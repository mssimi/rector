{
    "chmod": "0755",
    "directories-bin": ["bin"],
    "shebang": "#!/usr/bin/env php",
    "main": "bin/rector",
    "output": "rector.phar",
    "finder": [
        {
            "name": "*.{php,yml}",
            "exclude": ["Tests", "tests", "docs"],
            "in": ["vendor", "src", "packages"]
        }
    ],
    "compression": "GZ"
}
